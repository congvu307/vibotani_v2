
@{
    ViewBag.Title = "Sản phẩm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- search product-->
<div style="position:fixed;bottom:2rem;left:0.5rem;z-index:100;width:25%;">
    <div id="search__header">
        <button class="btn btn-success" style="border-radius:50%;width:40px;height:40px;" id="search_open"><i class="fas fa-search"></i></button>
        <button class="btn d-none pb-0" style="color:#28a745" id="search_close"><i class="fas fa-times"></i></button>
    </div>
    <div class=" d-none" id="search_container">
        <div class="input-group mb-2">
            <select style="border:3px solid #28a745;border-radius:0;display:block;" type="text" class="form-control">
                <option disabled selected>--Chọn dòng sản phẩm--</option>
                <option>Sản phẩm trắng da</option>
                <option>Sản phẩm đẹp da</option>
                <option>Mỹ phẩm</option>
                <option>Sản phẩm đặc trị</option>
            </select>
        </div>
        <input style="border:3px solid #28a745;border-radius:0;" type="text" class="mb-2 form-control" placeholder="Tìm kiếm sản phẩm" aria-label="Input group example" aria-describedby="btnGroupAddon">
        <button class="btn btn-success" style="border-radius:0;"><i class="fas fa-search"></i></button>
    </div>
</div>
<!--modal-product-detail-->
<div class="modal fade" id="modal-product-detail" tabindex="-1" role="dialog" aria-labelledby="modal-product" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content" style="border:none; position:relative;">
            <div class="modal-body p-0" style="border:none; position:relative;">
                <button type="button" class="close text-right modal-button" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <div class="row align-items-md-center m-0 bg-s">
                    <div class="col-md-5 mt-3 col-sm-12">
                        <img class="w-100 mb-3 image-main" src="" />
                        <div class="owl-carousel owl-theme p-2" id="product-detail-carousel">
                            <div class="item">
                                <img class="image-1" src="" />
                            </div>
                            <div class="item">
                                <img class="image-2" src="~/Content/Image/Intro/8U9A5229.png" />
                            </div>
                            <div class="item">
                                <img class="image-3" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-7 col-sm-12 pl-3 pr-5">
                        <div class="col-12 col-md-8 mx-auto">
                            <img class="w-100" src="~/Content/Image/Logo/Logo_BiOshi.png" />
                        </div>
                        <h3 class="text-green text-center title" style="font-weight:bold;">Whitening Face Cream </h3>
                        <h5 class="text-center text-green-s sub-title"><i>White beauty from Nature</i></h5>
                        <h5 class="text-green-s" style="font-weight:bold">
                            Thông tin sản phẩm
                        </h5>

                        <p class="text-green info" style="font-size:small;">

                        </p>

                        <h5 class="text-green-s" style="font-weight:bold">
                            Thành phần

                        </h5>
                        <p class="text-green  ingredient " style="font-size:small;">
                        </p>
                        <h5 class="text-green-s" style="font-weight:bold">
                            Hướng dẫn sử dụng

                        </h5>
                        <p class="text-green use" style="font-size:small;">
                        </p>
                        <h5>Giá bán lẻ đề nghị: <small class="text-green price"></small></h5>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<section>
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
